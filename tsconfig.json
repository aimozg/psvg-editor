    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": true,
        "rootDir": ".",
        "outDir": "tsout"
    },
    "include":[
        "ts/*.ts",
        "ts/**/*.ts",
        "typings/index.d.ts"
    ],
    "exclude": [
        "node_modules",
        "js",
        ".c9"
    ]
}